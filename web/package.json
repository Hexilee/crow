{
  "name": "crow",
  "version": "1.0.0",
  "description": "3D curve reconstruction based on fiber bragg grating",
  "main": "index.js",
  "author": "Li Chenxi",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@types/dat.gui": "^0.7.5",
    "@types/stats": "^0.16.30",
    "dat.gui": "^0.7.6",
    "google-protobuf": "^3.11.0",
    "grpc-web": "^1.0.7",
    "stats.js": "^0.17.0",
    "three": "^0.111.0",
    "three-dragcontrols": "^0.88.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "json-server": "^0.15.1",
    "parcel-bundler": "^1.12.4",
    "protoc-gen-grpc-web": "^1.0.5",
    "typescript": "^3.7.2"
  },
  "scripts": {
    "install": "yarn gen-js & yarn gen-ts",
    "gen-ts": "protoc -I=src/gen/grpc ../proto/crow.proto --grpc-web_out=import_style=typescript,mode=grpcweb:src/gen/grpc/ --proto_path=../proto/",
    "gen-js": "protoc -I=src/gen/grpc ../proto/crow.proto --js_out=import_style=commonjs:src/gen/grpc/ --proto_path=../proto/",
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "serve": "hs dist"
  }
}
